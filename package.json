{
  "name": "calendar-helper",
  "version": "1.0.0",
  "description": "Calendar helper library to use in calendars.",
  "main": "src/index.ts",
  "scripts": {
    "start": "nodemon",
    "build": "tsc",
    "prepare": "npm run build",
    "lint": "npx eslint \"**/*.{ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier \"**/*.{ts,tsx}\" --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix"
  },
  "author": "",
  "license": "ISC",
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "date-fns": "^2.28.0"
  },
  "devDependencies": {
    "@types/date-fns": "^2.6.0",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LucielSantos/calendar-helper.git"
  },
  "homepage": "https://github.com/LucielSantos/calendar-helper#readme"
}
